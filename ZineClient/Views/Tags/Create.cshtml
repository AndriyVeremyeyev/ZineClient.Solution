@model ZineClient.Models.Tag
@{
  ViewData["Title"] = "Add Tag";
}

<div class="text-center">
  <h1>@ViewData["Title"]</h1>
  <h2>Help us curate posts</h2>
</div>


<div class="registration-form">
  <form asp-controller="Tags" asp-action="Create" method="post">

    @* <input type="hidden" asp-for="@Model.OrganizationId" value="@ViewBag.OrganizationId"> *@

    <div class="form-group">
      <label for="Name">Name</label>
      <input class="form-control" asp-for="Name">
    </div>

    <input class="btn btn-light" type="submit" value="Create">
  </form>
</div>








